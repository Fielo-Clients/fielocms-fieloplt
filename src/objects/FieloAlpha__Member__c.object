    <validationRules>
        <fullName>FieloPLT__BirthDate</fullName>
        <active>true</active>
        <errorConditionFormula>FieloPLT__Birthdate__c &gt;  TODAY()</errorConditionFormula>
        <errorDisplayField>FieloPLT__Birthdate__c</errorDisplayField>
        <errorMessage>Birth date cannot be greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__CPF_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL( FieloPLT__IdentificationType__c , &apos;CPF&apos; ) , NOT(
OR(
  LEN( FieloPLT__IdentificationCode__c ) = 0,
  AND(
   LEN(FieloPLT__IdentificationCode__c) = 14,
   CONTAINS(&quot;0123456789&quot;, LEFT(FieloPLT__IdentificationCode__c, 1)),
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 2, 1)),
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 3, 1)),
   MID(FieloPLT__IdentificationCode__c, 4, 1) = &quot;.&quot;,
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 5, 1)),
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 6, 1)),
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 7, 1)),
   MID(FieloPLT__IdentificationCode__c, 8, 1) = &quot;.&quot;,
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 9, 1)),
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 10, 1)),
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 11, 1)),
   MID(FieloPLT__IdentificationCode__c, 12, 1) = &quot;-&quot;,
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 13, 1)),
   CONTAINS(&quot;0123456789&quot;, MID(FieloPLT__IdentificationCode__c, 14, 1))
))))</errorConditionFormula>
        <errorMessage>invalid CPF  999.999.999-99
Ex.: 478.497.338-94</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ContactMatchingField</fullName>
        <active>true</active>
        <description>This validation rule is useful when the non duplication contact process is triggered, in order to avoid duplicate contact due to have empty the common unique field (email)</description>
        <errorConditionFormula>ISBLANK(FieloPLT__Email__c) &amp;&amp; (RecordType.DeveloperName = &apos;OrganizationContributor&apos; || RecordType.DeveloperName = &apos;IndividualAccount&apos;)</errorConditionFormula>
        <errorDisplayField>FieloPLT__Email__c</errorDisplayField>
        <errorMessage>The email is required in order to avoid duplicate contacts</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ModifyProgram</fullName>
        <active>true</active>
        <description>Is not possible to modify the associated Program</description>
        <errorConditionFormula>ISCHANGED(FieloPLT__Program__c) &amp;&amp; NOT(ISBLANK(PRIORVALUE(FieloPLT__Program__c)))</errorConditionFormula>
        <errorDisplayField>FieloPLT__Program__c</errorDisplayField>
        <errorMessage>Is not possible to modify the associated Program</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__NegativePoints</fullName>
        <active>true</active>
        <description>A Member can not have negative points.</description>
        <errorConditionFormula>FieloPLT__Points__c &lt; 0</errorConditionFormula>
        <errorMessage>A Member can not have negative points.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__OrganizationAccountCanNotBeManager</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;OrganizationAccount&apos; &amp;&amp; FieloPLT__IsManager__c</errorConditionFormula>
        <errorDisplayField>FieloPLT__IsManager__c</errorDisplayField>
        <errorMessage>An Organization Account can not be a manager</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__OrganizationAccountRelation</fullName>
        <active>true</active>
        <description>Check if an Organization Account is being related to an other Organization Account</description>
        <errorConditionFormula>IF( AND(RecordType.DeveloperName == &apos;OrganizationAccount&apos;, NOT(ISBLANK(FieloPLT__OrganizationAccount__c)) ),
true,
false)</errorConditionFormula>
        <errorDisplayField>FieloPLT__OrganizationAccount__c</errorDisplayField>
        <errorMessage>An Organization Account can not related to an other Organization Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__RequiredCode</fullName>
        <active>true</active>
        <errorConditionFormula>( ISPICKVAL( FieloPLT__IdentificationType__c, &apos;&apos; ) &amp;&amp; ( FieloPLT__IdentificationCode__c != &apos;&apos; ) ) || ( ! ISPICKVAL( FieloPLT__IdentificationType__c, &apos;&apos; ) &amp;&amp; ( FieloPLT__IdentificationCode__c == &apos;&apos; ) )</errorConditionFormula>
        <errorMessage>You must complete Identification type and Identification Number or neither of both</errorMessage>
    </validationRules>
