        <deprecated>false</deprecated>
        <description>Link to the phisical attachment</description>
        <externalId>false</externalId>
        <formula>IF( 
    AND( 
        NOT( ISNULL(FieloPLT__AttachmentId__c) )
        , NOT( ISBLANK(FieloPLT__AttachmentId__c) ) 
    ) ,
      IMAGE(
            IF( CONTAINS(FieloPLT__AttachmentId__c, &apos;&amp;LastModifiedDate&apos;), LEFT(FieloPLT__AttachmentId__c, FIND(&apos;&amp;LastModifiedDate&apos;, FieloPLT__AttachmentId__c)), FieloPLT__AttachmentId__c)
            ,&quot;photo&quot;,50, 50),
    &apos;&apos;)</formula>
