        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}

var url = parent.location.href;
var levelUpdate = [];

var idLevel = &quot;{!FieloPLT__Level__c.Id}&quot;;
var resultLevel = sforce.connection.query(&quot;SELECT Id, {!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}IsActive__c, RecordTypeID FROM {!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}Level__c WHERE Id = \&apos;{!FieloPLT__Level__c.Id}\&apos; &quot;);

var level = resultLevel.getArray(&quot;records&quot;)[0];
level.{!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}IsActive__c = true;
levelUpdate.push(level);

result = sforce.connection.update(levelUpdate);
parent.location.href = idLevel;</url>
