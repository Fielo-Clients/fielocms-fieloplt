{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}


var menu = &apos;{!FieloCMS__Page__c.Id}&apos;;

var saveURL = &quot;/apex/&quot; + &quot;{!IF(!ISBLANK($Setup.FieloCMS__OrganizationSettings__c.FieloCMS__PackagePrefix__c), $Setup.FieloCMS__OrganizationSettings__c.FieloCMS__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}&quot; + &quot;PageClone&quot; + &quot;%3F&quot; + &quot;menuId=&quot; + menu;

var cancelURL = &quot;/apex/&quot; + &quot;{!IF(!ISBLANK($Setup.FieloCMS__OrganizationSettings__c.FieloCMS__PackagePrefix__c), $Setup.FieloCMS__OrganizationSettings__c.FieloCMS__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}&quot; + &quot;SiteLanding&quot;;

window.location = &quot;/{!FieloCMS__Page__c.Id}/e?clone=1&amp;saveURL=&quot;+saveURL+&quot;&amp;cancelURL=&quot;+cancelURL;</url>
    </webLinks>
