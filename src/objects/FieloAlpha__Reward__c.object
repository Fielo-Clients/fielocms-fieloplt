        <searchResultsAdditionalFields>FieloPLT__Stock__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>FieloPLT__PointsCant__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>FieloPLT__Available_Days</fullName>
        <active>true</active>
        <errorConditionFormula>FieloPLT__AvailableDays__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloPLT__AvailableDays__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ChangeStockMode</fullName>
        <active>true</active>
        <errorConditionFormula>((ISPICKVAL(PRIORVALUE(FieloPLT__VoucherCreationMode__c), &apos;Controlled By Quantity&apos;) &amp;&amp; (ISPICKVAL(FieloPLT__VoucherCreationMode__c, &apos;Unlimited&apos;) || ISPICKVAL(FieloPLT__VoucherCreationMode__c, &apos;Controlled By Item&apos;))) ||
(ISPICKVAL(PRIORVALUE(FieloPLT__VoucherCreationMode__c), &apos;Controlled By Item&apos;) &amp;&amp; (ISPICKVAL(FieloPLT__VoucherCreationMode__c, &apos;Unlimited&apos;) ||
ISPICKVAL(FieloPLT__VoucherCreationMode__c, &apos;Controlled By Quantity&apos;)))) &amp;&amp;
FieloPLT__Stock__c &gt; 0</errorConditionFormula>
        <errorDisplayField>FieloPLT__VoucherCreationMode__c</errorDisplayField>
        <errorMessage>This operation is allowed when stock is equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__CheckCategoryProgram</fullName>
        <active>true</active>
        <description>Check if the Program from the Category chosen</description>
        <errorConditionFormula>AND( NOT(ISBLANK(FieloPLT__Category__c)), FieloPLT__Category__r.FieloPLT__Program__c &lt;&gt; FieloPLT__Program__c)</errorConditionFormula>
        <errorDisplayField>FieloPLT__Category__c</errorDisplayField>
        <errorMessage>The Program of the Category must be the same as the Reward.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ModifyProgram</fullName>
        <active>true</active>
        <description>Is not possible to modify the associated Program</description>
        <errorConditionFormula>ISCHANGED(FieloPLT__Program__c) &amp;&amp; NOT(ISBLANK(PRIORVALUE(FieloPLT__Program__c)))</errorConditionFormula>
        <errorDisplayField>FieloPLT__Program__c</errorDisplayField>
        <errorMessage>Is not possible to modify the associated Program</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__MultiVoucher</fullName>
        <active>true</active>
        <errorConditionFormula>FieloPLT__UniqueVoucher__c &amp;&amp;  ISPICKVAL(FieloPLT__VoucherCreationMode__c, &apos;Controlled By Item&apos;)</errorConditionFormula>
        <errorMessage>Rewards controlled By Item with preloaded vouchers can&apos;t be unique voucher</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Points_Required</fullName>
        <active>true</active>
        <description>Validation to set allways a point value to the reward</description>
        <errorConditionFormula>ISNULL( FieloPLT__PointsCant__c )</errorConditionFormula>
        <errorDisplayField>FieloPLT__PointsCant__c</errorDisplayField>
        <errorMessage>The points of the reward are mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Price_Possitive</fullName>
        <active>true</active>
        <errorConditionFormula>FieloPLT__Price__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloPLT__Price__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Quantity_Points_Positive</fullName>
        <active>true</active>
        <errorConditionFormula>FieloPLT__PointsCant__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloPLT__PointsCant__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Stock_Positive</fullName>
        <active>true</active>
        <description>Stock Must be positive</description>
        <errorConditionFormula>AND(IF(ISCHANGED(FieloPLT__Stock__c) ,
IF( OR(ISBLANK(FieloPLT__Stock__c) , FieloPLT__Stock__c &lt; 0) ,
true,
false),
false), NOT( ISPICKVAL(FieloPLT__VoucherCreationMode__c, &apos;Unlimited&apos;) ))</errorConditionFormula>
        <errorMessage>The stock should be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Unlimited</fullName>
        <active>false</active>
        <errorConditionFormula>FieloPLT__Intangible__c = TRUE &amp;&amp;  ISPICKVAL( FieloPLT__VoucherCreationMode__c , &apos;Preload&apos; )</errorConditionFormula>
        <errorMessage>A reward cannot be unlimited and preload at the same time</errorMessage>
    </validationRules>
