        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Upload Photo</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/uploadPhoto?id={!FieloAlpha__Reward__c.Id}</url>
    </webLinks>
</CustomObject>
