        <active>true</active>
        <errorConditionFormula>FieloAlpha__AvailableDays__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloAlpha__AvailableDays__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__ChangeStockMode</fullName>
        <active>true</active>
        <errorConditionFormula>((ISPICKVAL(PRIORVALUE(FieloAlpha__VoucherCreationMode__c), &apos;Controlled By Quantity&apos;) &amp;&amp; (ISPICKVAL(FieloAlpha__VoucherCreationMode__c, &apos;Unlimited&apos;) || ISPICKVAL(FieloAlpha__VoucherCreationMode__c, &apos;Controlled By Item&apos;))) ||
(ISPICKVAL(PRIORVALUE(FieloAlpha__VoucherCreationMode__c), &apos;Controlled By Item&apos;) &amp;&amp; (ISPICKVAL(FieloAlpha__VoucherCreationMode__c, &apos;Unlimited&apos;) ||
ISPICKVAL(FieloAlpha__VoucherCreationMode__c, &apos;Controlled By Quantity&apos;)))) &amp;&amp;
FieloAlpha__Stock__c &gt; 0</errorConditionFormula>
        <errorDisplayField>FieloAlpha__VoucherCreationMode__c</errorDisplayField>
        <errorMessage>This operation is allowed when stock is equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__CheckCategoryProgram</fullName>
        <active>true</active>
        <description>Check if the Program from the Category chosen</description>
        <errorConditionFormula>AND( NOT(ISBLANK(FieloAlpha__Category__c)), FieloAlpha__Category__r.FieloAlpha__Program__c &lt;&gt; FieloAlpha__Program__c)</errorConditionFormula>
        <errorDisplayField>FieloAlpha__Category__c</errorDisplayField>
        <errorMessage>The Program of the Category must be the same as the Reward.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__ModifyProgram</fullName>
        <active>true</active>
        <description>Is not possible to modify the associated Program</description>
        <errorConditionFormula>ISCHANGED(FieloAlpha__Program__c) &amp;&amp; NOT(ISBLANK(PRIORVALUE(FieloAlpha__Program__c)))</errorConditionFormula>
        <errorDisplayField>FieloAlpha__Program__c</errorDisplayField>
        <errorMessage>Is not possible to modify the associated Program</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__MultiVoucher</fullName>
        <active>true</active>
        <errorConditionFormula>FieloAlpha__UniqueVoucher__c &amp;&amp;  ISPICKVAL(FieloAlpha__VoucherCreationMode__c, &apos;Controlled By Item&apos;)</errorConditionFormula>
        <errorMessage>Rewards controlled By Item with preloaded vouchers can&apos;t be unique voucher</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__Points_Required</fullName>
        <active>true</active>
        <description>Validation to set allways a point value to the reward</description>
        <errorConditionFormula>ISNULL( FieloAlpha__PointsCant__c )</errorConditionFormula>
        <errorDisplayField>FieloAlpha__PointsCant__c</errorDisplayField>
        <errorMessage>The points of the reward are mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__Price_Possitive</fullName>
        <active>true</active>
        <errorConditionFormula>FieloAlpha__Price__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloAlpha__Price__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__Quantity_Points_Positive</fullName>
        <active>true</active>
        <errorConditionFormula>FieloAlpha__PointsCant__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloAlpha__PointsCant__c</errorDisplayField>
        <errorMessage>value must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__Stock_Positive</fullName>
        <active>true</active>
        <description>Stock Must be positive</description>
        <errorConditionFormula>AND(IF(ISCHANGED(FieloAlpha__Stock__c) ,
IF( OR(ISBLANK(FieloAlpha__Stock__c) , FieloAlpha__Stock__c &lt; 0) ,
true,
false),
false), NOT( ISPICKVAL(FieloAlpha__VoucherCreationMode__c, &apos;Unlimited&apos;) ))</errorConditionFormula>
        <errorMessage>The stock should be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloAlpha__Unlimited</fullName>
        <active>false</active>
        <errorConditionFormula>FieloAlpha__Intangible__c = TRUE &amp;&amp;  ISPICKVAL( FieloAlpha__VoucherCreationMode__c , &apos;Preload&apos; )</errorConditionFormula>
        <errorMessage>A reward cannot be unlimited and preload at the same time</errorMessage>
    </validationRules>
