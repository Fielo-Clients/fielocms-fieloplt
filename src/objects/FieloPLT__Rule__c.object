    <validationRules>
        <fullName>FieloPLT__EmptyRuleName</fullName>
        <active>true</active>
        <description>Rule name must be completed.</description>
        <errorConditionFormula>ISBLANK( Name )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Rule name must be completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ValidatePointsValue</fullName>
        <active>true</active>
        <description>Points cannot be empty or 0.</description>
        <errorConditionFormula>AND( OR( ISPICKVAL(FieloPLT__TransactionConversionMode__c,&apos;Fixed Value&apos;), ISPICKVAL(FieloPLT__TransactionConversionMode__c,&apos;Factor&apos;), ISPICKVAL(FieloPLT__TransactionConversionMode__c,&apos;Multiplier&apos;) ), OR(FieloPLT__ConversionValue__c == 0, ISNULL(FieloPLT__ConversionValue__c), ISBLANK(FieloPLT__ConversionValue__c)) )</errorConditionFormula>
        <errorMessage>Points cannot be empty or 0.</errorMessage>
    </validationRules>
