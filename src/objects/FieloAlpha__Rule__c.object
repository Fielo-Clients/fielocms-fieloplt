if(confirm(&apos;{!$Label.FieloPLT__Fielo_ChangeStatus}?&apos;)){
     var c = new sforce.SObject(&quot;{!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}Rule__c&quot;);
     var toUpdate = [];
     c.id = &apos;{!FieloPLT__Rule__c.Id}&apos;;
     c.{!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}IsActive__c = {!IF(FieloPLT__Rule__c.FieloPLT__IsActive__c, &apos;false&apos;, &apos;true&apos;)};
    toUpdate.push(c);
    sforce.connection.update(toUpdate);

    window.location.reload();
}</url>
