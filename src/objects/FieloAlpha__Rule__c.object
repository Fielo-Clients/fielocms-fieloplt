    <validationRules>
        <fullName>FieloPLT__Rule_Name_Empty</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Name )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Name must be completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Validate_points_value</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( ISPICKVAL(FieloPLT__ConvertionTypeTransaction__c,&apos;Fix Value&apos;), ISPICKVAL(FieloPLT__ConvertionTypeTransaction__c,&apos;Factor&apos;), ISPICKVAL(FieloPLT__ConvertionTypeTransaction__c,&apos;Multiplier&apos;) ), OR(FieloPLT__ConvertionValue__c == 0, ISNULL(FieloPLT__ConvertionValue__c), ISBLANK(FieloPLT__ConvertionValue__c)) )</errorConditionFormula>
        <errorMessage>The quantity of points cannot be empty or be 0</errorMessage>
    </validationRules>
