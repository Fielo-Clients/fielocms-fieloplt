    <webLinks>
        <fullName>FieloPLT__SetDefault</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Set Default</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}

var programUpdate = [];

var idProgram = &quot;{!FieloPLT__Program__c.Id}&quot;;
var resultProgram = sforce.connection.query(&quot;SELECT Id, FieloPLT__IsDefault__c FROM FieloPLT__Program__c WHERE Id = \&apos;{!FieloPLT__Program__c.Id}\&apos; &quot;);

var program = resultProgram.getArray(&quot;records&quot;)[0];
program.FieloPLT__IsDefault__c = &apos;true&apos;;
programUpdate.push(program);

if(confirm(&apos;{!$Label.FieloPLT__ProgramSetDefault}&apos;)){
  result = sforce.connection.update(programUpdate);
  parent.location.href = idProgram;
}</url>
    </webLinks>
