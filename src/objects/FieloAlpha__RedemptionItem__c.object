{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

var prefix = &apos;{!$Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c}&apos;.replace(&apos;__&apos;, &apos;.&apos;);

