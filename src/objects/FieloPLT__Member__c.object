    <validationRules>
        <fullName>FieloPLT__ContactMatchingField</fullName>
        <active>true</active>
        <description>This validation rule is useful when the non duplication contact process is triggered, to avoid duplicate contact due to an empty common unique field (email).</description>
        <errorConditionFormula>ISBLANK(FieloPLT__Email__c) &amp;&amp; (RecordType.DeveloperName = &apos;OrganizationContributor&apos; || RecordType.DeveloperName = &apos;IndividualAccount&apos;)</errorConditionFormula>
        <errorDisplayField>FieloPLT__Email__c</errorDisplayField>
        <errorMessage>Email is required to avoid Contact duplication.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ModifyProgram</fullName>
        <active>true</active>
        <description>The related Program cannot be modified.</description>
        <errorConditionFormula>ISCHANGED(FieloPLT__Program__c) &amp;&amp; NOT(ISBLANK(PRIORVALUE(FieloPLT__Program__c)))</errorConditionFormula>
        <errorDisplayField>FieloPLT__Program__c</errorDisplayField>
        <errorMessage>The related Program cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__NegativePoints</fullName>
        <active>true</active>
        <description>Member cannot have negative points.</description>
        <errorConditionFormula>FieloPLT__Points__c &lt; 0</errorConditionFormula>
        <errorMessage>Member cannot have negative points.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__OrganizationAccountCanNotBeManager</fullName>
        <active>true</active>
        <description>An Organization Account cannot be set as Manager.</description>
        <errorConditionFormula>RecordType.DeveloperName = &apos;OrganizationAccount&apos; &amp;&amp; FieloPLT__IsManager__c</errorConditionFormula>
        <errorDisplayField>FieloPLT__IsManager__c</errorDisplayField>
        <errorMessage>An Organization Account cannot be set as Manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__OrganizationAccountRelation</fullName>
        <active>true</active>
        <description>An Organization Account cannot  be related to another Organization Account.</description>
        <errorConditionFormula>IF( AND(RecordType.DeveloperName == &apos;OrganizationAccount&apos;, NOT(ISBLANK(FieloPLT__OrganizationAccount__c)) ),
true,
false)</errorConditionFormula>
        <errorDisplayField>FieloPLT__OrganizationAccount__c</errorDisplayField>
        <errorMessage>An Organization Account cannot  be related to another Organization Account.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>FieloPLT__BlockUnblock</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Block/Unblock</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

if( confirm(&apos;{!$Label.FieloPLT__ConfirmBlockUnblock}&apos;) ){
    var ids = [&apos;{!FieloPLT__Member__c.Id}&apos;];
    var action = &apos;&apos;;

    if( &apos;{!FieloPLT__Member__c.FieloPLT__IsBlocked__c}&apos; == 1 ){
        action = &apos;false&apos;;
    } else {
        action = &apos;true&apos;;
    }

    var status = sforce.apex.execute(prefix+&apos;MemberService&apos;, &apos;blockUnblock&apos;, {memberIds:ids, action:action});
    window.location.reload();
}</url>
    </webLinks>
