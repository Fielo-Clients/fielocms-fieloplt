    <validationRules>
        <fullName>FieloCMS__ErrorInvalidElementTypeForChild</fullName>
        <active>true</active>
        <description>The Element Type for a child Section cannot be Header, Main or Footer.</description>
        <errorConditionFormula>FieloCMS__Parent__r.FieloCMS__Page__r.RecordType.DeveloperName = &apos;Template&apos; &amp;&amp; 
NOT(ISNULL(FieloCMS__Parent__c)) &amp;&amp; 
((ISPICKVAL(FieloCMS__ElementType__c, &apos;Header&apos;)) || 
(ISPICKVAL(FieloCMS__ElementType__c, &apos;Footer&apos;)) || 
(ISPICKVAL(FieloCMS__ElementType__c, &apos;Main&apos;)))</errorConditionFormula>
        <errorDisplayField>FieloCMS__ElementType__c</errorDisplayField>
        <errorMessage>The Element Type for a child Section cannot be Header, Main or Footer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCMS__ErrorInvalidElementTypeForPage</fullName>
        <active>true</active>
        <description>This Element Type is not valid for a Page.</description>
        <errorConditionFormula>(FieloCMS__Page__r.RecordType.DeveloperName = &apos;Page&apos; ||
FieloCMS__Parent__r.FieloCMS__Page__r.RecordType.DeveloperName = &apos;Page&apos;)
&amp;&amp; 
(ISPICKVAL(FieloCMS__ElementType__c, &apos;Header&apos;) ||
ISPICKVAL(FieloCMS__ElementType__c, &apos;Main&apos;) ||
ISPICKVAL(FieloCMS__ElementType__c, &apos;Footer&apos;))</errorConditionFormula>
        <errorDisplayField>FieloCMS__ElementType__c</errorDisplayField>
        <errorMessage>This Element Type is not valid for a Page.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCMS__ErrorInvalidElementTypeForParent</fullName>
        <active>true</active>
        <description>The Element Type for a parent Section must be Header, Main or Footer.</description>
        <errorConditionFormula>FieloCMS__Page__r.RecordType.DeveloperName = &apos;Template&apos; &amp;&amp; 
ISNULL(FieloCMS__Parent__c) &amp;&amp;  
NOT(ISPICKVAL(FieloCMS__ElementType__c, &apos;Header&apos;)) &amp;&amp; 
NOT(ISPICKVAL(FieloCMS__ElementType__c, &apos;Footer&apos;)) &amp;&amp; 
NOT(ISPICKVAL(FieloCMS__ElementType__c, &apos;Main&apos;))</errorConditionFormula>
        <errorDisplayField>FieloCMS__ElementType__c</errorDisplayField>
        <errorMessage>The Element Type for a parent Section must be Header, Main or Footer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCMS__ErrorThirdLevelSection</fullName>
        <active>true</active>
        <description>A Section cannot be a child of another child section.</description>
        <errorConditionFormula>IF(NOT(ISBLANK(FieloCMS__Parent__c)), NOT(ISBLANK(FieloCMS__Parent__r.FieloCMS__Parent__c)), false)</errorConditionFormula>
        <errorDisplayField>FieloCMS__Parent__c</errorDisplayField>
        <errorMessage>A Section cannot be a child of another child section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCMS__OrphanSections</fullName>
        <active>true</active>
        <description>The sections/subsections must be child of a Page/Section.</description>
        <errorConditionFormula>ISBLANK(FieloCMS__Parent__c) &amp;&amp; ISBLANK( FieloCMS__Page__c )</errorConditionFormula>
        <errorMessage>The sections/subsections must be child of a Page/Section.</errorMessage>
    </validationRules>
