        <masterLabel>Active</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}

var url = parent.location.href;
var agreementUpdate = [];

var idAgreement= &quot;{!FieloPLT__Agreement__c.Id}&quot;;
var resultAgreement = sforce.connection.query(&quot;SELECT Id, {!IF(!isBLank($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}Status__c FROM {!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}Agreement__c WHERE Id = \&apos;{!FieloPLT__Agreement__c.Id}\&apos; &quot;);

var agreement = resultAgreement.getArray(&quot;records&quot;)[0];
agreement.{!IF(!isBLank($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}Status__c = &apos;Current&apos;;
agreementUpdate.push(agreement);
result = sforce.connection.update(agreementUpdate);

parent.location.href = idAgreement;</url>
    </webLinks>
