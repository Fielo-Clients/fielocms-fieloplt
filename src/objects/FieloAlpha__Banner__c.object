    <validationRules>
        <fullName>FieloPLT__ActiveBanner</fullName>
        <active>false</active>
        <errorConditionFormula>( ISNULL( FieloPLT__AttachmentId__c ) || ISBLANK( FieloPLT__AttachmentId__c ) ) &amp;&amp; (FieloPLT__isActive__c == true)</errorConditionFormula>
        <errorMessage>You must load an image to active a banner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__EndDategreaterthanStartDate</fullName>
        <active>true</active>
        <errorConditionFormula>FieloPLT__From__c &gt; FieloPLT__To__c</errorConditionFormula>
        <errorDisplayField>FieloPLT__From__c</errorDisplayField>
        <errorMessage>To Date must be greater than From Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ModifyProgram</fullName>
        <active>true</active>
        <description>Is not possible to modify the associated Program</description>
        <errorConditionFormula>ISCHANGED(FieloPLT__Program__c) &amp;&amp; NOT(ISBLANK(PRIORVALUE(FieloPLT__Program__c)))</errorConditionFormula>
        <errorDisplayField>FieloPLT__Program__c</errorDisplayField>
        <errorMessage>Is not possible to modify the associated Program</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Name_Required</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISNULL(Name) ,  ISBLANK(Name) )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Name Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Order_can_not_be_negative</fullName>
        <active>true</active>
        <errorConditionFormula>FieloPLT__Order__c &lt; 0</errorConditionFormula>
        <errorDisplayField>FieloPLT__Order__c</errorDisplayField>
        <errorMessage>The order can not be negative</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>FieloPLT__ActiveInactive</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Active/Inactive</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/connection.js&quot;)}

var bannerUpdate = [];

var resultBanner = sforce.connection.query(&quot;SELECT Id, {!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}isActive__c FROM {!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}Banner__c WHERE Id = \&apos;{!FieloPLT__Banner__c.Id}\&apos; &quot;);

var banner = resultBanner.getArray(&quot;records&quot;)[0];
if(banner.{!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}isActive__c == &apos;true&apos;){
    banner.{!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}isActive__c = &apos;false&apos;;
}else{
    banner.{!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}isActive__c = &apos;true&apos;;
}
bannerUpdate.push(banner);

result = sforce.connection.update(bannerUpdate);

if(result.toString().indexOf(&quot;errors:&quot;) != -1){
    alert(result[0].errors.message);
}
parent.location.href = &apos;{!FieloPLT__Banner__c.Id}&apos;;</url>
