        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}

var defaultRules = sforce.connection.query(&quot;SELECT Id FROM {!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}Rule__c WHERE {!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}Program__c = \&apos;{!FieloPLT__Promotion__c.FieloPLT__ProgramId__c}\&apos; AND RecordType.DeveloperName = \&apos;Default_Rule\&apos; &quot;);
var defaultRule = defaultRules.getArray(&quot;records&quot;)[0];

window.open(&apos;/apex/{!IF(!ISBLANK($Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c), $Setup.FieloPLT__PublicSettings__c.FieloPLT__PackagePrefix__c + &apos;__&apos;,&apos;&apos;)}RuleDefaultEditBackEnd?id=&apos; + defaultRule.Id);</url>
